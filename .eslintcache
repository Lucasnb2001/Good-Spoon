io\\React\\Complete-React-Tutorial-lesson-3\\dojo-blog\\src\\Home.js",["52"],"/* eslint-disable linebreak-style */\r\nimport React from 'react';\r\nimport BlogList from './BlogList';\r\nimport useFetch from './useFetch';\r\n\r\nconst Home = function () {\r\n  const { error, isPending, data: blogs } = useFetch('http://localhost:8000/recipes');\r\n  return (\r\n    <div className=\"home\">\r\n      { error && <div>{error}</div>}\r\n      { isPending && <div>Loading...</div> }\r\n      <BlogList title=\"Recipes\" blogs={blogs} />\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default Home;\r\n","C:\\Users\\Adriano Vieira\\Documents\\Estágio\\React\\Complete-React-Tutorial-lesson-3\\dojo-blog\\src\\BlogList.js",["53"],"/* eslint-disable linebreak-style */\r\n/* eslint-disable react/jsx-one-expression-per-line */\r\n/* eslint-disable react/jsx-indent */\r\n/* eslint-disable react/prop-types */\r\n/* eslint-disable linebreak-style */\r\nimport React from 'react';\r\nimport { Link } from 'react-router-dom';\r\n\r\nconst BlogList = function (props) {\r\n  const { blogs } = props;\r\n  const { title } = props;\r\n  return (\r\n    <div className=\"blog-list\">\r\n        <h3>{title}</h3>\r\n        {blogs.map((element) => (\r\n        <div className=\"element-preview\" key={element.id}>\r\n        <Link to={`/details/${element.id}`}>\r\n        <h2>{element.title }</h2>\r\n        <p>{element.body}</p>\r\n        <p>Likes: {element.likes}</p>\r\n        </Link>\r\n        </div>\r\n        ))}\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default BlogList;\r\n","C:\\Users\\Adriano Vieira\\Documents\\Estágio\\React\\Complete-React-Tutorial-lesson-3\\dojo-blog\\src\\useFetch.js",["54","55"],"/* eslint-disable linebreak-style */\r\nimport { useState, useEffect } from 'react';\r\n\r\nconst useFetch = function (url) {\r\n  const [data, setData] = useState([]);\r\n  const [isPending, setIsPending] = useState(true);\r\n  const [error, setError] = useState(null);\r\n\r\n  useEffect(() => {\r\n    const abortCont = new AbortController();\r\n\r\n    fetch(url, { signal: abortCont.signal })\r\n      .then((res) => {\r\n        if (!res.ok) { // error coming back from server\r\n          throw Error('could not fetch the data for that resource');\r\n        }\r\n        return res.json();\r\n      })\r\n      .then((dado) => {\r\n        setIsPending(false);\r\n        setData(dado);\r\n        setError(null);\r\n      })\r\n      .catch((err) => {\r\n        // auto catches network / connection error\r\n        if (err.name === 'AbortError') {\r\n          console.log('fetch aborted');\r\n        } else {\r\n          // auto catches network / connection error\r\n          setIsPending(false);\r\n          setError(err.message);\r\n        }\r\n      });\r\n\r\n    return () => abortCont.abort();\r\n  }, [url]);\r\n\r\n  return { data, isPending, error };\r\n};\r\n\r\nexport default useFetch;\r\n","C:\\Users\\Adriano Vieira\\Documents\\Estágio\\React\\Complete-React-Tutorial-lesson-3\\dojo-blog\\src\\Create.js",[],"C:\\Users\\Adriano Vieira\\Documents\\Estágio\\React\\Complete-React-Tutorial-lesson-3\\dojo-blog\\src\\RecipeDetails.js",["56","57"],"/* eslint-disable linebreak-style */\r\n/* eslint-disable react/jsx-one-expression-per-line */\r\n/* eslint-disable prefer-template */\r\nimport { React, useState, useEffect } from 'react';\r\nimport { useParams, useHistory, Link } from 'react-router-dom';\r\nimport useFetch from './useFetch';\r\n\r\nconst RecipeDetais = function () {\r\n  const [contLikes, setCont] = useState(0);\r\n  const [canUpdate, setCanUpdate] = useState(false);\r\n  const { id } = useParams();\r\n  const { data: recipe, error, isPending } = useFetch('http://localhost:8000/recipes/' + id);\r\n  const history = useHistory();\r\n\r\n  useEffect(() => {\r\n    const dataHora = new Date();\r\n    console.log(dataHora.toLocaleString(), ' ', recipe);\r\n    // eslint-disable-next-line no-prototype-builtins\r\n    if (recipe.hasOwnProperty('likes')) {\r\n      setCont(recipe.likes);\r\n    }\r\n  }, [recipe]);\r\n\r\n  const handleClick = () => {\r\n    fetch('http://localhost:8000/recipes/' + recipe.id, {\r\n      method: 'DELETE',\r\n    }).then(() => {\r\n      history.push('/');\r\n    });\r\n  };\r\n\r\n  useEffect(() => {\r\n    const obj = { title: recipe.title, body: recipe.body, likes: contLikes };\r\n    if (canUpdate) {\r\n      fetch('http://localhost:8000/recipes/' + recipe.id, {\r\n        method: 'PUT',\r\n        headers: { 'Content-Type': 'application/json' },\r\n        body: JSON.stringify(obj),\r\n      });\r\n      setCanUpdate(false);\r\n    }\r\n  }, [contLikes, recipe, canUpdate]);\r\n\r\n  const handleLike = () => {\r\n    setCont(contLikes + 1);\r\n    setCanUpdate(true);\r\n  };\r\n\r\n  const handleDislike = () => {\r\n    setCont(contLikes - 1);\r\n    setCanUpdate(true);\r\n  };\r\n\r\n  return (\r\n    <div className=\"recipe-details\">\r\n      { isPending && <div>Loading...</div> }\r\n      { error && <div>{ error }</div> }\r\n      { recipe && (\r\n        <article>\r\n          <h2>{ recipe.title }</h2>\r\n          <p>Likes: {contLikes}</p>\r\n          <div>{ recipe.body }</div>\r\n          <Link to={`/edit/${recipe.id}`}><button type=\"button\">edit</button></Link>\r\n          <button type=\"button\" onClick={handleClick}>delete</button>\r\n          <button type=\"button\" onClick={handleLike}>like</button>\r\n          <button type=\"button\" onClick={handleDislike}>dislike</button>\r\n        </article>\r\n      )}\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default RecipeDetais;\r\n","C:\\Users\\Adriano Vieira\\Documents\\Estágio\\React\\Complete-React-Tutorial-lesson-3\\dojo-blog\\src\\Update.js",["58","59"],"/* eslint-disable linebreak-style */\r\n/* eslint-disable no-prototype-builtins */\r\n/* eslint-disable prefer-template */\r\n/* eslint-disable jsx-a11y/label-has-associated-control */\r\n/* eslint-disable react/jsx-indent */\r\n\r\nimport { React, useState, useEffect } from 'react';\r\nimport { useParams, useHistory } from 'react-router-dom';\r\nimport useFetch from './useFetch';\r\n\r\nconst Update = function () {\r\n  const { id } = useParams();\r\n  const { data: recipe, error, isPending } = useFetch('http://localhost:8000/recipes/' + id);\r\n  const [title, setTitle] = useState('');\r\n  const [body, setBody] = useState('');\r\n  const history = useHistory();\r\n\r\n  useEffect(() => {\r\n    const dataHora = new Date();\r\n    console.log(dataHora.toLocaleString(), ' ', recipe);\r\n    if (recipe.hasOwnProperty('title') && recipe.hasOwnProperty('body')) {\r\n      setTitle(recipe.title);\r\n      setBody(recipe.body);\r\n    }\r\n  }, [recipe]);\r\n\r\n  const handleEdit = async (e) => {\r\n    e.preventDefault();\r\n    const newRecipe = { title, body, likes: recipe.likes };\r\n\r\n    await fetch('http://localhost:8000/recipes/' + id, {\r\n      method: 'PUT',\r\n      headers: { 'Content-Type': 'application/json' },\r\n      body: JSON.stringify(newRecipe),\r\n    }).then(() => {\r\n      history.push('/');\r\n    });\r\n  };\r\n  return (\r\n    <div className=\"create\">\r\n      { isPending && <div>Loading...</div> }\r\n      { error && <div>{ error }</div> }\r\n      { (recipe && !error) && (\r\n          <article>\r\n          <div>\r\n          <h2>Edit the Recipe</h2>\r\n            <form onSubmit={handleEdit}>\r\n            <label>Recipe title:</label>\r\n            <input\r\n              type=\"text\"\r\n              required\r\n              value={title}\r\n              onChange={(e) => setTitle(e.target.value)}\r\n            />\r\n            <label>Recipe body:</label>\r\n            <textarea\r\n              required\r\n              value={body}\r\n              onChange={(e) => setBody(e.target.value)}\r\n            />\r\n            <button type=\"submit\">update Recipe</button>\r\n            </form>\r\n          </div>\r\n          </article>\r\n      )}\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default Update;\r\n",{"ruleId":"60","severity":1,"message":"61","line":5,"column":16,"nodeType":"62","messageId":"63","endLine":5,"endColumn":25},{"ruleId":"64","replacedBy":"65"},{"ruleId":"66","replacedBy":"67"},{"ruleId":"68","replacedBy":"69"},{"ruleId":"70","replacedBy":"71"},{"ruleId":"72","replacedBy":"73"},{"ruleId":"74","replacedBy":"75"},{"ruleId":"60","severity":1,"message":"61","line":6,"column":14,"nodeType":"62","messageId":"63","endLine":6,"endColumn":23},{"ruleId":"60","severity":1,"message":"61","line":9,"column":18,"nodeType":"62","messageId":"63","endLine":9,"endColumn":27},{"ruleId":"60","severity":1,"message":"61","line":4,"column":18,"nodeType":"62","messageId":"63","endLine":4,"endColumn":27},{"ruleId":"76","severity":1,"message":"77","line":27,"column":11,"nodeType":"78","messageId":"79","endLine":27,"endColumn":22},{"ruleId":"60","severity":1,"message":"61","line":8,"column":22,"nodeType":"62","messageId":"63","endLine":8,"endColumn":31},{"ruleId":"76","severity":1,"message":"77","line":17,"column":5,"nodeType":"78","messageId":"79","endLine":17,"endColumn":16},{"ruleId":"60","severity":1,"message":"61","line":11,"column":16,"nodeType":"62","messageId":"63","endLine":11,"endColumn":25},{"ruleId":"76","severity":1,"message":"77","line":20,"column":5,"nodeType":"78","messageId":"79","endLine":20,"endColumn":16},"func-names","Unexpected unnamed function.","FunctionExpression","unnamed","lines-around-directive",["80"],"no-spaced-func",["81"],"global-require",[],"no-buffer-constructor",[],"no-new-require",[],"no-path-concat",[],"no-console","Unexpected console statement.","MemberExpression","unexpected","padding-line-between-statements","func-call-spacing"]